{
  "build": {
    "development": {
      "developmentClient": true,
      "channel": "development",
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "image": "latest"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "ios": {
        "resourceClass": "m-medium",
        "image": "latest"
      },
      "android": {
        "image": "latest"
      }
    },
    "android-apk": {
      "android": {
        "buildType": "apk"
      }
    },
    "android-aab": {
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "cli": {
    "appVersionSource": "local",
    "promptToConfigurePushNotifications": false
  }
}